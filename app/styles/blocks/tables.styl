table
	border-collapse collapse
	width 100%
	margin-bottom 8px

th
	background-color #c7d3db
	text-align right
	height 18px

td
	text-align right
	border-bottom 1px solid #000
	font-size 15px

td:first-child
	text-align left

.bold
	font-weight bold

.orange_border_tb > td
	border-top 1px solid $orange
	border-bottom 1px solid $orange

.orange_border_b > td
	border-bottom 1px solid $orange

.border_dotted_l
	border-left 1px dotted $orange


/*table class 05-1(page ID + table ID on page)*/
