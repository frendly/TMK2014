+script([], [
	'libs/jquery/dist/jquery.min.js',
	'libs/fotorama/fotorama.js',
	'libs/fancybox/source/jquery.fancybox.pack.js',
	'libs/jquery-sticky/jquery.sticky.js',
	'libs/cookies-js/dist/cookies.min.js',
	'libs/jquery-pjax/jquery.pjax.js',
	'common.min.js'
])

head
	meta(charset='utf-8')
	meta(name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1, minimal-ui')
	meta(http-equiv='X-UA-Compatible' content='IE=edge')


	block head
	title= pageTitle
	meta(name='description' content=pageDescription)
	meta(name='keywords' content=pageKeywords)
	meta(name='copyright' content=pageCopyright)

	link(href=globalPath + 'assets/styles/common.css?' + timestamp rel='stylesheet')
